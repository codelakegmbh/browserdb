    "type": "git",
    "url": "git+https://github.com/codelakegmbh/local-database.git"
  },
  "keywords": [
    "localstorage",
    "browser"
  ],
  "author": "JÃ¶rn Neumeyer",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/codelakegmbh/local-database/issues"
  },
  "homepage": "https://github.com/codelakegmbh/local-database#readme",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "typescript": "^3.3.4000",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "testRegex": "\\.spec\\.ts$",
